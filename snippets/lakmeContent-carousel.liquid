<div class="grid text-center contentWidget contentWidgetContainer contentWidget-carousel contentWidget__{{ widgetSerial }} hr-ml-custom">
    <div class="grid__item contentWidget-heading">
        {% if contentWidget.heading != blank %}
            <span class="h5 contentWidget-heading">{{ contentWidget.heading }}</span>
        {% endif %}
    </div>
    <div class="contentWidget-content">
        {% assign firstImage = contentWidget.images[0] %}
            <div class="grid__item">
                <div id="mid_section">
                    <h3 class="text-middle carousel-title"></h3>
                    <div class="extra-small-screen-12 small-screen-12 medium-screen-offset-2 medium-screen-8 caro-outer-outer">
                        <div class="extra-small-screen-1 small-screen-2 medium-screen-1 text-middle arrow-outer"><img
                                    class="arrow" data="left"
                                    src="https://cdn.shopify.com/s/files/1/0014/3514/0183/t/11/assets/absolute-nudes-left-arrow.png"
                                    alt="" /></div>
                        <div class="extra-small-screen-10 small-screen-8 medium-screen-10 text-middle no-pad caro-main">
                            <img class="caro-img"
                                 src="{{ firstImage.image }}"
                                 alt="" />
                            {% if firstImage.swatchImage != blank %}
                            <div class="swatch-data"><img class="caro-swatch"
                                                          src="{{ firstImage.swatchImage }}"
                                                          alt="" /></div>
                            {% endif %}
                            <div class="clearfix"></div>
                        </div>
                        <div class="extra-small-screen-1 small-screen-2 medium-screen-1 text-middle arrow-outer"><img
                                    class="arrow" data="right"
                                    src="https://cdn.shopify.com/s/files/1/0014/3514/0183/t/11/assets/absolute-nudes-right-arrow.png"
                                    alt="" /></div>
                    </div>

                    <div class="clearfix"></div>
                    <img class="top-bg" src="https://cdn.shopify.com/s/files/1/0014/3514/0183/t/11/assets/absolute-nudes-top-bg.png" alt="" />
                </div>
                <div class="hides">
                    {% for contentImage in contentWidget.images %}
                        <img src="{{ contentImage.image }}" alt="{{ contentImage.altText }}" >
                        <img src="{{ contentImage.swatchImage }}" alt="" >
                    {% endfor %}
                </div>
            </div>
    </div>
</div>
<script>// <![CDATA[
    $(window).on('load', function () {
        var sliderIndex = 0;
        var carouselData = {{ contentWidget.images | json }};

        $('.arrow').click(function () {
            var direction = $(this).attr('data');
            if (direction == "left") {
                sliderIndex--;
            } else {
                sliderIndex++;
            }

            if (sliderIndex < 0) {
                sliderIndex = carouselData.length - 1;
            }

            if (sliderIndex == carouselData.length) {
                sliderIndex = 0;
            }

            $('.caro-img,.caro-name,.caro-swatch,.caro-link').fadeOut(function () {
                $('.caro-img').attr('src', 'https://cdn.shopify.com/s/files/1/0014/3514/0183/t/11/assets/'
                    + carouselData[sliderIndex].image);
                $('.caro-swatch').attr('src', 'https://cdn.shopify.com/s/files/1/0014/3514/0183/t/11/assets/'
                    + carouselData[sliderIndex].swatchImage);
                $('.caro-link').attr('href', carouselData[sliderIndex].link);
                $('.caro-img,.caro-name,.caro-swatch,.caro-link').fadeIn();
            });
        });
    });
    // ]]></script>
